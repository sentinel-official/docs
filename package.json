    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "docusaurus-plugin-typedoc": "^1.0.1",
    "eslint": "^9.4.0",
    "eslint-plugin-react": "^7.34.2",
    "husky": "^9.0.11",
    "is-ci": "^3.0.1",
    "jest": "^29.7.0",
    "pascal-case": "^4.0.0",
    "path-browserify": "^1.0.1",
    "prettier": "^3.3.1",
